{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "$(APPLE_ID)",
        "ascAppId": "$(ASC_APP_ID)",
        "appleTeamId": "$(APPLE_TEAM_ID)"
      }
    }
  }
} 